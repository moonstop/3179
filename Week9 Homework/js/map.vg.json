{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    
      "width": 400,
      "height": 400,
    "data": {
        "url": "https://raw.githubusercontent.com/moonstop/3179/main/Week9%20Homework/js/output.json",
        "format": {
            "type": "topojson",
            "feature": "ne_110m_admin_0_countries"
        }
    },
    "projection": {
        "type": "equalEarth"
    },
    "layer": [
        {
            "mark": {
                "type": "geoshape",
                "fill": "lightgray",
                "stroke": "white"
            },
            "encoding": {
                "tooltip": {
                    "field": "note"
                }
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/moonstop/3179/main/Week9%20Homework/js/output.json",
                "format": {
                    "type": "topojson",
                    "feature": "oceans"
                }
            },
            "mark": {
                "type": "geoshape",
                "fill": "skyblue"
            }
        },
        {
            "data": {
                "url": "https://raw.githubusercontent.com/moonstop/3179/main/Week9%20Homework/js/output.json",
                "format": {
                    "type": "topojson",
                    "feature": "ne_110m_graticules_30"
                }
            },
            "mark": {
                "type": "geoshape",
                "fill": null,
                "stroke": "lightgray"
            }
        },
        {
            "transform": [
                {
                    "lookup": "properties.NAME",
                    "from": {
                        "data": {
                            "url": "https://raw.githubusercontent.com/moonstop/3179/main/Week9%20Homework/data/country_wise_latest.csv"
                        },
                        "key": "Country/Region",
                        "fields": [
                            "Confirmed"]
                    }
                },
                {"calculate": "datum.Confirmed/10000", "as": "Confirmed Cases per 10000"} 
            ],
            "mark": {
                "type": "geoshape",
                "stroke": "white"
            },
            "encoding": {
          
                "color": {
                    "field": "Confirmed Cases per 10000",
                    "type": "quantitative"
                },
                "tooltip": [
                    {
                        "field": "properties.NAME",
                        "type": "nominal",
                        "title": "Country"
                    },
                    {
                        "field": "Confirmed Cases per 10000",
                        "type": "quantitative"
                    }
                ]
            }
        }
    ]
  }